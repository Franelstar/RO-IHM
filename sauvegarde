digraph Flot {
	S;
	P;
	Personne1;
	Personne2;
	Personne3;
	T1;
	T2;
	T3;
	S -> Personne1 [label=1];
	S -> Personne2 [label=1];
	S -> Personne3 [label=1];
	Personne1 -> T1 [label=1];
	Personne1 -> T2 [label=1];
	Personne2 -> T2 [label=1];
	Personne2 -> T3 [label=1];
	Personne3 -> T1 [label=1];
	Personne3 -> T2 [label=1];
	Personne3 -> T3 [label=1];
	T1 -> P [label=1];
	T2 -> P [label=1];
	T3 -> P [label=1];
}